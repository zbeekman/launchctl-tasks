<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/X11/bin:/usr/local/MacGPG2/bin:/Library/TeX/texbin:/Applications/Wireshark.app/Contents/MacOS:/usr/local/sbin</string>
	</dict>
	<key>Label</key>
	<string>sh.brew.cask.outdated</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>-c</string>
		<string>rm /tmp/cask.outdated ; /usr/local/bin/brew cask outdated &gt; /tmp/cask.outdated</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/caskoutdated.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/cask.outdated</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Minute</key>
		<integer>5</integer>
	</dict>
</dict>
</plist>
